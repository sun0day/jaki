{
  "name": "jaki.pattern",
  "type": "module",
  "version": "0.0.0",
  "private": false,
  "description": "ast pattern for jaki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sun0day/jaki.git"
  },
  "keywords": [
    "jaki",
    "ast",
    "toolkit",
    "pattern",
    "ecmascript",
    "typescript",
    "ts-pattern"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test:type": "tsc --noEmit"
  },
  "devDependencies": {
    "ts-pattern": "^5.2.0"
  }
}
